#include <iostream>

using namespace std;

int main ()

{
	int col, x;
	
	cout << "Enter number of columns: ";
	cin >> col;
	
	cout << "Enter number of elements: ";
	cin >> x;
	
	string array[col][x];
	
	for (int i = 0; i < col; i++)
	{
		cout << "Column " << i + 1 << endl;
		for (int j = 0; j < x; j++)
		{
			cout << "Element " << j + 1 << ": ";
			cin >> array[i][j];
		}
	}
	cout << "\nTWO DIMENSIONAL ARRAYS ELEMENTS\n";
	for (int i = 0; i < col; i++) {
		cout << "Column " << i + 1 << endl;
		for (int j = 0; j < x; j++)
		{
			cout << "Element " << j + 1 << ": ";
			cout << array[i][j];
			cout << endl;
		}
	}
    return 0;
}
