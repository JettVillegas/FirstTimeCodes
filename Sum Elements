#include <iostream>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};  // an array of integers
    int n = sizeof(arr) / sizeof(arr[0]);  // calculate the length of the array

    int sum = 0;  // initialize the sum variable to 0
    for (int i = 0; i < n; i++) {
        sum += arr[i];  // add each element to the sum variable
    }

    cout << "The sum of the elements in the array is: " << sum << endl;

    return 0;
}
